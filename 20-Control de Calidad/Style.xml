<blockTableStyle id="TablaTitulo">
    <!-- Borde superior (fila 0) -->
    <lineStyle kind="LINEABOVE" colorName="#000" start="0,0" stop="3,0"/>    
    <!-- Borde inferior (última fila, por ejemplo fila 4) -->
    <lineStyle kind="LINEBELOW" colorName="#000" start="0,4" stop="3,4"/>    
    <!-- Borde izquierdo -->
    <lineStyle kind="LINEBEFORE" colorName="#000" start="0,0" stop="0,4"/>    
    <!-- Borde derecho -->
    <lineStyle kind="LINEAFTER" colorName="#000" start="3,0" stop="3,4"/>
</blockTableStyle>

<blockTableStyle id="marc_photo">
    <!-- Estilo de línea solo para la segunda celda -->
<lineStyle thickness="0.5" kind="GRID" colorName="#000" start="1,0" stop="1,-1"/> 
</blockTableStyle>

